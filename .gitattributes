* text=auto

# Explicitly declare text files you want to always be normalized and converted
# to native line endings on checkout.
*.c text eol=lf
*.cc text eol=lf
*.cpp text eol=lf
*.h text eol=lf
*.hh text eol=lf
*.hpp text eol=lf
*.inl text eol=lf
*.py text eol=lf

*.bat text eol=crlf

# Built files
*.lib binary
*.dll binary
*.zip binary